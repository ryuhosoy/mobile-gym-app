{
  "expo": {
    "name": "ジム検索アプリ",
    "slug": "gym-search-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon_1024x1024.png",
    "scheme": "gymapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "description": "近くのジムを検索できるアプリ",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ryuhosoy.gym-app",
      "buildNumber": "1",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "近くのジムを検索するために位置情報を使用します",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "近くのジムを検索するために位置情報を使用します",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 250,
          "resizeMode": "contain",
          "backgroundColor": "#fefdfd"
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "jsEngine": "hermes",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d1f274ad-506c-4d9c-a166-36c0a26e3147"
      }
    }
  }
}
